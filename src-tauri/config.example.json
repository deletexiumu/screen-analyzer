{
  "llm": {
    "qwen": {
      "api_key": "sk-xxxxxxxxxxxx"
      // model, base_url, use_video_mode 都有默认值
    },
    "analysis_params": {
      "frame_sampling_interval": 30,
      "max_frames_per_analysis": 15,
      "include_detailed_description": true,
